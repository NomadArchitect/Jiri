[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[infra::cipd]/resources/bootstrap.py",
      "--platform",
      "linux-amd64",
      "--dest-directory",
      "[SLAVE_BUILD]/cipd",
      "--json-output",
      "/path/to/tmp/json"
    ],
    "name": "install cipd",
    "~followup_annotations": [
      "@@@STEP_TEXT@cipd instance_id: 40-chars-fake-of-the-package-instance_id@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"executable\": \"[SLAVE_BUILD]/cipd/cipd\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"instance_id\": \"40-chars-fake-of-the-package-instance_id\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[SLAVE_BUILD]/cipd/cipd",
      "ensure",
      "--root",
      "[CACHE]/cipd",
      "--list",
      "fuchsia/tools/jiri/linux-amd64 latest",
      "--json-output",
      "/path/to/tmp/json"
    ],
    "name": "ensure_installed",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"instance_id\": \"resolved-instance_id-of-latest----------\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"package\": \"fuchsia/tools/jiri/linux-amd64\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[CACHE]/cipd/jiri",
      "init"
    ],
    "name": "init"
  },
  {
    "cmd": [
      "[CACHE]/cipd/jiri",
      "project",
      "clean"
    ],
    "name": "project clean"
  },
  {
    "cmd": [
      "[CACHE]/cipd/jiri",
      "import",
      "-overwrite=true",
      "jiri",
      "https://fuchsia.googlesource.com/manifest"
    ],
    "name": "import"
  },
  {
    "cmd": [
      "[CACHE]/cipd/jiri",
      "update",
      "-autoupdate=false",
      "-gc=true"
    ],
    "name": "update"
  },
  {
    "cmd": [
      "[CACHE]/cipd/jiri",
      "cl",
      "patch",
      "-host",
      "https://https://fuchsia-review.googlesource.com",
      "-delete=true",
      "-force=true",
      "refs/changes/11/338811/3"
    ],
    "name": "patch"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[infra::go]/resources/bootstrap.py",
      "--platform",
      "linux-amd64",
      "--dest-directory",
      "[SLAVE_BUILD]/go",
      "--json-output",
      "/path/to/tmp/json"
    ],
    "name": "install go",
    "~followup_annotations": [
      "@@@STEP_TEXT@Go version: go1.7@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"path\": \"[SLAVE_BUILD]/go/go\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"version\": \"go1.7\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "show",
      "HEAD",
      "--format=%H",
      "-s"
    ],
    "cwd": "[SLAVE_BUILD]/go/src/fuchsia.googlesource.com/jiri",
    "name": "git show",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "date",
      "--rfc-3339=seconds"
    ],
    "name": "date",
    "stdout": "/path/to/tmp/"
  },
  {
    "cmd": [
      "[SLAVE_BUILD]/go/go/bin/go",
      "build",
      "-ldflags",
      "-X \"fuchsia.googlesource.com/jiri/version.GitCommit=deadbeef\" -X \"fuchsia.googlesource.com/jiri/version.BuildTime=2016-10-11 14:40:25-07:00\"",
      "-a",
      "fuchsia.googlesource.com/jiri/cmd/jiri"
    ],
    "env": {
      "GOARCH": "amd64",
      "GOOS": "linux",
      "GOPATH": "[SLAVE_BUILD]/go",
      "GOROOT": "[SLAVE_BUILD]/go/go"
    },
    "name": "go build"
  },
  {
    "cmd": [
      "[SLAVE_BUILD]/go/go/bin/go",
      "test",
      "fuchsia.googlesource.com/jiri/cmd/jiri"
    ],
    "env": {
      "GOPATH": "[SLAVE_BUILD]/go",
      "GOROOT": "[SLAVE_BUILD]/go/go"
    },
    "name": "go test"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]